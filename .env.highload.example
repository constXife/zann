ZANN_TRACING_OTEL_ENABLED=true
ZANN_TRACING_OTEL_ENDPOINT=https://otlp.arkham.void/v1/traces
ZANN_TRACING_OTEL_SERVICE_NAME=zann-server-loadtest
ZANN_TRACING_OTEL_SAMPLING_RATIO=0.02
ZANN_TRACING_OTEL_CA_FILE=/run/ca/arkham-ca.crt
RUST_LOG=zann_server=debug,zann_db=debug,zann_core=debug,tower_http=info,sqlx=warn
# Optional jemalloc heap profiles (requires build with --features jemalloc).
# MALLOC_CONF=prof:true,prof_active:false is needed for heap dumps.
# ZANN_HEAP_PROFILE=1
# ZANN_HEAP_PROFILE_DIR=/data/heap
